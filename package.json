{
  "name": "collabedit-node-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "seed": "tsx src/seed.ts",
    "cleanup:minio": "tsx src/scripts/cleanup-minio.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "jsonwebtoken": "latest",
    "minio": "latest",
    "morgan": "latest",
    "multer": "latest",
    "uuid": "latest"
  },
  "devDependencies": {
    "@types/cors": "latest",
    "@types/express": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/morgan": "latest",
    "@types/multer": "latest",
    "@types/node": "latest",
    "prisma": "^6.19.2",
    "tsx": "latest",
    "typescript": "latest"
  }
}
